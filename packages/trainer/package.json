{
  "name": "dwang-in-de-zorg",
  "version": "0.0.1",
  "description": "Capture, store and share your learned lessons in a simple to use web application.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public ./.cache ./dist",
    "cleanup": "npm run clean",
    "dev": "set SERVER=http://localhost:3030&& parcel index.html",
    "start": "parcel index.html",
    "build:domain": "npm run clean && parcel build index.html --out-dir ./public",
    "link": "npm link mithril-ui-form",
    "serve": "rest-easy-loki",
    "keycloak": "docker-compose up -d -f ./docker/docker-compose.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/dwang-in-de-zorg.git"
  },
  "keywords": [
    "SPA",
    "mithril",
    "location register",
    "REST",
    "database",
    "lokijs",
    "dynamic",
    "forms"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/dwang-in-de-zorg/issues"
  },
  "homepage": "https://github.com/erikvullings/dwang-in-de-zorg#readme",
  "dependencies": {
    "base64-js": "^1.3.1",
    "file-saver": "^2.0.2",
    "js-sha256": "^0.9.0",
    "keycloak-js": "^8.0.1",
    "leaflet": "^1.6.0",
    "leaflet-draw": "^1.0.4",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril": "^2.0.4",
    "mithril-stream": "2.0.0",
    "mithril-leaflet": "^0.1.7",
    "mithril-materialized": "^0.16.0",
    "mithril-ui-form": "^0.4.9",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/leaflet": "^1.5.8",
    "@types/lokijs": "^1.5.3",
    "@types/materialize-css": "^1.0.7",
    "@types/mithril": "^2.0.2",
    "@types/socket.io-client": "^1.4.32",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.5"
  }
}
